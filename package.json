{
    "name": "mix-dom",
    "version": "3.2.0",
    "description": "Powerful state based DOM rendering library (in JS/TS)",
    "keywords": [
        "MixDOM",
        "mix-dom",
        "mix",
        "mixing",
        "components",
        "dom",
        "render",
        "react",
        "user interface",
        "virtual dom",
        "front-end",
        "library",
        "framework"
    ],
    "homepage": "https://mixdomjs.org",
    "author": "Mikael Saur√©n",
    "license": "MIT",
    "private": false,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mixdomjs/mixdom.git"
    },
    "module": "./dist/MixDOM.module.js",
    "main": "./dist/MixDOM.js",
    "types": "./dist/MixDOM.d.ts",
    "exports": {
        ".": {
            "import": "./dist/MixDOM.module.js",
            "require": "./dist/MixDOM.js"
        },
        "./package.json": "./package.json"
    },
    "files": [
        "dist",
        "src",
        "CONTRIBUTING.md"
    ],
    "scripts": {
        "tsc": "tsc -p tsconfig.json",
        "build": "tsc && rollup -c rollup.config.js"
    },
    "devDependencies": {
        "rollup": "^2.77.2",
        "rollup-plugin-copy": "^3.4.0",
        "rollup-plugin-delete": "^2.0.0",
        "rollup-plugin-dts": "^4.2.2",
        "rollup-plugin-terser": "^7.0.2",
        "typescript": "^4.8.2"
    },
    "dependencies": {}
}
